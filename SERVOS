#include <Servo.h>

// Define el pin al que est치 conectado el servo
int servoPin = 9;

// Crea un objeto Servo para controlar el servo motor
Servo miServo;

void setup() {
  // Inicializa el servo en el pin especificado
  miServo.attach(servoPin);
}

void loop() {
  // Mueve el servo a la posici칩n 0 grados
  miServo.write(0);
  delay(1000); // Espera 1 segundo

  // Mueve el servo a la posici칩n 90 grados
  miServo.write(90);
  delay(1000); // Espera 1 segundo

  // Mueve el servo a la posici칩n 180 grados
  miServo.write(180);
  delay(1000); // Espera 1 segundo
}
